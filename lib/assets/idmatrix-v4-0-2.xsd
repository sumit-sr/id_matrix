											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:element name="address" maxOccurs="unbounded">
														<xs:annotation>
															<xs:documentation>Residential address</xs:documentation>
														</xs:annotation>
														<xs:complexType>
															<xs:complexContent>
																<xs:extension base="idm:addressType-no-restrictions"/>
															</xs:complexContent>
														</xs:complexType>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
									<xs:attribute name="record-create-date" type="xs:date" use="optional">
										<xs:annotation>
											<xs:documentation>Date the record matched was created in the dataset
											</xs:documentation>
										</xs:annotation>
									</xs:attribute>
								</xs:complexType>
							</xs:element>
							<xs:element name="visa-details-record" minOccurs="0" maxOccurs="1">
								<xs:annotation>
									<xs:documentation>Details of visa for non Australian passport holder matched during vevo search.</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="visa-class-subclass" nillable="true">
											<xs:annotation>
												<xs:documentation>The class and sub class of the visa.</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:simpleContent>
													<xs:extension base="xs:token">
														<xs:attributeGroup ref="idm:id-attributes"/>
													</xs:extension>
												</xs:simpleContent>
											</xs:complexType>
										</xs:element>
										<xs:element name="visa-applicant" nillable="true">
											<xs:annotation>
												<xs:documentation>Whether the person is the primary or secondary applicant on the visa.</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:simpleContent>
													<xs:extension base="xs:string">
														<xs:attributeGroup ref="idm:id-attributes"/>
													</xs:extension>
												</xs:simpleContent>
											</xs:complexType>
										</xs:element>
										<xs:element name="visa-date-of-grant" minOccurs="0" maxOccurs="1">
											<xs:annotation>
												<xs:documentation>Date that the visa was granted</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:simpleContent>
													<xs:extension base="xs:date">
														<xs:attributeGroup ref="idm:id-attributes"/>
													</xs:extension>
												</xs:simpleContent>
											</xs:complexType>
										</xs:element>
										<xs:element name="visa-date-of-expiry" minOccurs="0" maxOccurs="1">
											<xs:annotation>
												<xs:documentation>Date that the visa will expire</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:simpleContent>
													<xs:extension base="xs:date">
														<xs:attributeGroup ref="idm:id-attributes"/>
													</xs:extension>
												</xs:simpleContent>
											</xs:complexType>
										</xs:element>
										<xs:element name="visa-work-entitlement" nillable="true">
											<xs:annotation>
												<xs:documentation>The visa holders rights to work in Australia</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:simpleContent>
													<xs:extension base="xs:string">
														<xs:attributeGroup ref="idm:id-attributes"/>
													</xs:extension>
												</xs:simpleContent>
											</xs:complexType>
										</xs:element>
										<xs:element name="location" nillable="true">
											<xs:annotation>
												<xs:documentation>Whether the pasport holder is onshore or offshore.</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:simpleContent>
													<xs:extension base="xs:string">
														<xs:attributeGroup ref="idm:id-attributes"/>
													</xs:extension>
												</xs:simpleContent>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
									<xs:attributeGroup ref="idm:id-attributes"/>
								</xs:complexType>
							</xs:element>
							<xs:element name="accuity-results-record" minOccurs="0" maxOccurs="unbounded">
								<xs:annotation>
									<xs:documentation>The details of any matches returned by the accuity service.  Accuity web service does not provide a schema for XML messages, therefore all fields have been declared as option to ensure that unusual responses from Accuity do not contradict the IDM schema.</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="match" minOccurs="0" maxOccurs="unbounded">
											<xs:annotation>
												<xs:documentation>Details of each match returned from Accuity.</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:element name="entity" minOccurs="0" maxOccurs="unbounded">
														<xs:annotation>
															<xs:documentation>Top level accuity entity.</xs:documentation>
														</xs:annotation>
														<xs:complexType>
															<xs:sequence>
																<xs:element name="entity-details" minOccurs="0" maxOccurs="1">
																	<xs:annotation>
																		<xs:documentation>Second level accuity entity</xs:documentation>
																	</xs:annotation>
																	<xs:complexType>
																		<xs:sequence>
																			<xs:element name="name" minOccurs="1" maxOccurs="1" nillable="true">
																				<xs:annotation>
																					<xs:documentation>The name of the entity</xs:documentation>
																				</xs:annotation>
																				<xs:complexType>
																					<xs:simpleContent>
																						<xs:extension base="xs:string"/>
																					</xs:simpleContent>
																				</xs:complexType>
																			</xs:element>
																			<xs:element name="list-id" minOccurs="1" maxOccurs="1" nillable="true">
																				<xs:annotation>
																					<xs:documentation>The id of the list the entity is on</xs:documentation>
																				</xs:annotation>
																				<xs:complexType>
																					<xs:simpleContent>
																						<xs:extension base="xs:string"/>
																					</xs:simpleContent>
																				</xs:complexType>
																			</xs:element>
																			<xs:element name="list-code" minOccurs="1" maxOccurs="1" nillable="true">
																				<xs:annotation>
																					<xs:documentation>The code of the list the entity is on</xs:documentation>
																				</xs:annotation>
																				<xs:complexType>
																					<xs:simpleContent>
																						<xs:extension base="xs:string"/>
																					</xs:simpleContent>
																				</xs:complexType>
																			</xs:element>
																			<xs:element name="entity-type" minOccurs="1" maxOccurs="1" nillable="true">
																				<xs:annotation>
																					<xs:documentation>The type of entity</xs:documentation>
																				</xs:annotation>
																				<xs:complexType>
																					<xs:simpleContent>
																						<xs:extension base="xs:string"/>
																					</xs:simpleContent>
																				</xs:complexType>
																			</xs:element>
																			<xs:element name="created-date" minOccurs="0" maxOccurs="1">
																				<xs:annotation>
																					<xs:documentation>The date that the entity was created</xs:documentation>
																				</xs:annotation>
																				<xs:complexType>
																					<xs:simpleContent>
																						<xs:extension base="xs:date"/>
																					</xs:simpleContent>
																				</xs:complexType>
																			</xs:element>
																			<xs:element name="last-update-date" minOccurs="0" maxOccurs="1">
																				<xs:annotation>
																					<xs:documentation>The date that the entity was last updated</xs:documentation>
																				</xs:annotation>
																				<xs:complexType>
																					<xs:simpleContent>
																						<xs:extension base="xs:date"/>
																					</xs:simpleContent>
																				</xs:complexType>
																			</xs:element>
																			<xs:element name="title" minOccurs="1" maxOccurs="1" nillable="true">
																				<xs:annotation>
																					<xs:documentation>The title of the entity</xs:documentation>
																				</xs:annotation>
																				<xs:complexType>
																					<xs:simpleContent>
																						<xs:extension base="xs:string"/>
																					</xs:simpleContent>
																				</xs:complexType>
																			</xs:element>
																			<xs:element name="source" minOccurs="1" maxOccurs="1" nillable="true">
																				<xs:annotation>
																					<xs:documentation>The source of the entity</xs:documentation>
																				</xs:annotation>
																				<xs:complexType>
																					<xs:simpleContent>
																						<xs:extension base="xs:string"/>
																					</xs:simpleContent>
																				</xs:complexType>
																			</xs:element>
																			<xs:element name="country" minOccurs="1" maxOccurs="1" nillable="true">
																				<xs:annotation>
																					<xs:documentation>The country code of residence of the entity</xs:documentation>
																				</xs:annotation>
																				<xs:complexType>
																					<xs:simpleContent>
																						<xs:extension base="xs:string"/>
																					</xs:simpleContent>
																				</xs:complexType>
																			</xs:element>
																			<xs:element name="country-name" minOccurs="1" maxOccurs="1" nillable="true">
																				<xs:annotation>
																					<xs:documentation>The country name of the residence of the entity</xs:documentation>
																				</xs:annotation>
																				<xs:complexType>
																					<xs:simpleContent>
																						<xs:extension base="xs:string"/>
																					</xs:simpleContent>
																				</xs:complexType>
																			</xs:element>
																			<xs:element name="sdf" minOccurs="0" maxOccurs="unbounded" nillable="true">
																				<xs:annotation>
																					<xs:documentation>Other information about the entity</xs:documentation>
																				</xs:annotation>
																				<xs:complexType>
																					<xs:simpleContent>
																						<xs:extension base="xs:string">
																							<xs:attribute name="name" type="xs:token" use="required"/>
																						</xs:extension>
																					</xs:simpleContent>
																				</xs:complexType>
																			</xs:element>
																		</xs:sequence>
																		<xs:attribute name="id" type="xs:int" use="optional">
																			<xs:annotation>
																				<xs:documentation>The Accuity Id of the entity.
																				</xs:documentation>
																			</xs:annotation>
																		</xs:attribute>
																		<xs:attribute name="version" type="xs:string" use="optional">
																			<xs:annotation>
																				<xs:documentation>The version of the entity.
																				</xs:documentation>
																			</xs:annotation>
																		</xs:attribute>
																	</xs:complexType>
																</xs:element>
															</xs:sequence>
															<xs:attribute name="id" type="xs:int" use="optional">
																<xs:annotation>
																	<xs:documentation>The Accuity Id of the entity.
																	</xs:documentation>
																</xs:annotation>
															</xs:attribute>
															<xs:attribute name="source" type="xs:string" use="optional">
																<xs:annotation>
																	<xs:documentation>The source of the entity.
																	</xs:documentation>
																</xs:annotation>
															</xs:attribute>
															<xs:attribute name="version" type="xs:string" use="optional">
																<xs:annotation>
																	<xs:documentation>The version of the entity.
																	</xs:documentation>
																</xs:annotation>
															</xs:attribute>
														</xs:complexType>
													</xs:element>
												</xs:sequence>
												<xs:attribute name="match-type" type="xs:string" use="optional">
													<xs:annotation>
														<xs:documentation>The match type</xs:documentation>
													</xs:annotation>
												</xs:attribute>
												<xs:attribute name="match-on" type="xs:string" use="optional">
													<xs:annotation>
														<xs:documentation>The field that the match was found on</xs:documentation>
													</xs:annotation>
												</xs:attribute>
												<xs:attribute name="rule-matched" use="optional">
													<xs:annotation>
														<xs:documentation>Y or N to indicate if there was a rule match</xs:documentation>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:pattern value="[YN]"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:attribute>
												<xs:attribute name="stop-descriptor" type="xs:string" use="optional">
													<xs:annotation>
														<xs:documentation>The stop descriptor</xs:documentation>
													</xs:annotation>
												</xs:attribute>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
									<!-- ClientId has deliberately been stripped from the Accuity response -->
									<xs:attribute name="search-result-name" type="xs:token" use="required">
										<xs:annotation>
											<xs:documentation>Name of the search that found the accuity details.</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="id" type="xs:int" use="optional">
										<xs:annotation>
											<xs:documentation>The id of the response.</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="is-hit" use="optional">
										<xs:annotation>
											<xs:documentation>Y or N to indicate if there is a hit on the search</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:pattern value="[YN]"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:attribute>
									<xs:attribute name="total-blocked" type="xs:int" use="optional">
										<xs:annotation>
											<xs:documentation>The count of blocked matches</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="total-changed" type="xs:int" use="optional">
										<xs:annotation>
											<xs:documentation>The count of changed matches</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="total-good" type="xs:int" use="optional">
										<xs:annotation>
											<xs:documentation>The count of good matches</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="total-matches" type="xs:int" use="optional">
										<xs:annotation>
											<xs:documentation>The count of total matches</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="total-open" type="xs:int" use="optional">
										<xs:annotation>
											<xs:documentation>The count of total open</xs:documentation>
										</xs:annotation>
									</xs:attribute>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:simpleType name="kba-sequence-noType">
		<xs:annotation>
			<xs:documentation>A sequence number used in KBA for identifying questions and the answer options.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:int">
			<xs:minInclusive value="1"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="kba-time-to-answerType">
		<xs:annotation>
			<xs:documentation>The time in seconds it took the applicant to answer a question.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:int">
			<xs:minInclusive value="0"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="kba-question-scoreType">
		<xs:annotation>
			<xs:documentation>The score for a individual KBA question.  This has one decimal place and allows values from 0 to 100.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:decimal">
			<xs:totalDigits value="4"/>
			<xs:fractionDigits value="1"/>
			<xs:maxInclusive value="100"/>
			<xs:minInclusive value="0"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="kba-match-indicatorType">
		<xs:annotation>
			<xs:documentation>An enumeration listing the possible values for the KBA match-indicator field.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:token">
			<xs:enumeration value="MATCH">
				<xs:annotation>
					<xs:documentation>The applicant has satisifed the requirements set for establishing identity using KBA.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="NO-MATCH">
				<xs:annotation>
					<xs:documentation>A applicant has not satisfied the requirements set for establishing identity using KBA.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="kba-question-generation-statusType">
		<xs:annotation>
			<xs:documentation>An enumeration for the status of question generation.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:token">
			<xs:enumeration value="ERROR">
				<xs:annotation>
					<xs:documentation>An error occured that prevented the system from generating questions for KBA.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="TIMEOUT">
				<xs:annotation>
					<xs:documentation>The system was not able to produce the minimum number of questions within the available time.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="MINIMUM-QUESTIONS-NOT-GENERATED">
				<xs:annotation>
					<xs:documentation>The system was unable to generate the minimum number of questions required to undertake KBA.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="MAXIMUM-QUESTIONS-NOT-GENERATED">
				<xs:annotation>
					<xs:documentation>The system was able to generate the minimum number of questions for KBA but was not able to generate the maximum number of questions for KBA.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="OK">
				<xs:annotation>
					<xs:documentation>The system generated the questions for KBA without warnings or errors.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="kba-scoring-statusType">
		<xs:annotation>
			<xs:documentation>An enumeration for the status of the scoring of answers.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:token">
			<xs:enumeration value="ERROR">
				<xs:annotation>
					<xs:documentation>An error occured that prevented the system from scoring the answers provided to KBA.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="UNSCORED">
				<xs:annotation>
					<xs:documentation>The answers have not been completed therefore the system has not attempted to calculate the KBA score.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="PARTIALLY-SCORED">
				<xs:annotation>
					<xs:documentation>The answers have not been completed therefore the system has not attempted to calculate the KBA score.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="FULLY-SCORED">
				<xs:annotation>
					<xs:documentation>The system successfully scored the answers.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="kba-question-statusType">
		<xs:annotation>
			<xs:documentation>The status of a KBA question.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:token">
			<xs:enumeration value="UNANSWERED">
				<xs:annotation>
					<xs:documentation>A question has been generated and passed to the client but not answered.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="ANSWERED">
				<xs:annotation>
					<xs:documentation>A question has been answered by the client.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="INVALID-ANSWER">
				<xs:annotation>
					<xs:documentation>The client has submitted an invalid answer to a question. eg selecting a non existent response option.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="kba-overall-scoreType">
		<xs:annotation>
			<xs:documentation>This is the overall score calculated for the questionaire based on the answers that the client has provided. This has one decimal place and allows values from 0 to 100.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:decimal">
			<xs:totalDigits value="4"/>
			<xs:fractionDigits value="1"/>
			<xs:maxInclusive value="100"/>
			<xs:minInclusive value="0"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="kba-questionnaireType">
		<xs:annotation>
			<xs:documentation>A list of questions for a client undertaking KBA.</xs:documentation>
		</xs:annotation>
		<xs:sequence >
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element name="multiple-choice-question" type="idm:kba-multiple-choice-questionType"/>
				<xs:element name="true-false-question" type="idm:kba-true-false-questionType"/>
				<xs:element name="free-text-question" type="idm:kba-free-text-questionType"/>
			</xs:choice>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="kba-questionType">
		<xs:annotation>
			<xs:documentation>A questions for a client undertaking KBA.  The system will not score the answer to each question as this would reveal information about the client.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="question-text" type="xs:string">
				<xs:annotation>
					<xs:documentation>A text of the question to ask the applicant.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="user-response-text" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>This is the text of the applicants answer to this question.  This will be the free text that the applicant typed in or the text of the response option that the applicant selected.  This will element will only be present when the question has been answered by the applicant.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="match-indicator" type="idm:kba-match-indicatorType" minOccurs="0"/>
			<xs:element name="score" type="idm:kba-question-scoreType" minOccurs="0"/>
		</xs:sequence>
		<xs:attribute name="sequence-no" type="idm:kba-sequence-noType" use="required"/>
		<xs:attribute name="status" type="idm:kba-question-statusType" use="required"/>
		<xs:attribute name="status-message" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation>Contains any warning generated while scoring the users response to the question.  Usually around the fact that changing the answer to a question is not allowed.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="time-to-answer" type="idm:kba-time-to-answerType" use="optional"/>
	</xs:complexType>
	<xs:complexType name="kba-response-optionType">
		<xs:annotation>
			<xs:documentation>The responses to multichoice question (this includes yes no questions).</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="option-text" type="xs:string">
				<xs:annotation>
					<xs:documentation>The text of the question to ask the user.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="sequence-no" type="idm:kba-sequence-noType" use="required"/>
	</xs:complexType>
	<xs:complexType name="kba-multiple-choice-questionType">
		<xs:annotation>
			<xs:documentation>A kba multiple choice question.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
	    	<xs:extension base="idm:kba-questionType">
		  		<xs:sequence>
		    		<xs:element name="response-option" type="idm:kba-response-optionType" maxOccurs="unbounded"/>
		  		</xs:sequence>
	    	</xs:extension>
	  	</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="kba-free-text-questionType">
		<xs:annotation>
			<xs:documentation>A kba free text question.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
	    	<xs:extension base="idm:kba-questionType"/>
	  	</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="kba-true-false-questionType">
		<xs:annotation>
			<xs:documentation>A kba true false type question.</xs:documentation>
		</xs:annotation>
	  	<xs:complexContent>
			<xs:extension base="idm:kba-questionType">
				<xs:sequence>
		    		<xs:element name="response-option" type="idm:kba-response-optionType" minOccurs="2" maxOccurs="2"/>
		  		</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="knowledge-based-authentication-response">
		<xs:complexType>
			<xs:annotation>
				<xs:documentation>ComplexType for knowledge based authentication response details</xs:documentation>
			</xs:annotation>
			<xs:sequence>
				<xs:element name="questionnaire" type="idm:kba-questionnaireType" />
				<xs:element name="score" type="idm:kba-overall-scoreType"/>
				<xs:element name="threshold" type="idm:kba-overall-scoreType"/>
				<xs:element name="match-indicator" type="idm:kba-match-indicatorType"/>
			</xs:sequence>
			<xs:attribute name="question-generation-status" type="idm:kba-question-generation-statusType" use="required" />
			<xs:attribute name="question-generation-message" type="xs:string" use="required" />
			<xs:attribute name="scoring-status" type="idm:kba-scoring-statusType" use="required" />
			<xs:attribute name="scoring-message" type="xs:string" use="required" />
		</xs:complexType>
	</xs:element>
	<xs:complexType name="kba-answerType">
		<xs:annotation>
			<xs:documentation>Contains the applicants response to a question.  It must be the text of the response to a free text question or the id of the response option for multichoice or yes not questions.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:choice minOccurs="0">
				<xs:element name="response-option-seq-no-selected" type="idm:kba-sequence-noType">
					<xs:annotation>
						<xs:documentation>The sequence number of the response option that the user selected for a yes-no or mult-choice question.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="free-text-answer" type="xs:string">
					<xs:annotation>
						<xs:documentation>Text response from the user to a free text question</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:choice>
		</xs:sequence>
		<xs:attribute name="question-seq-no" type="idm:kba-sequence-noType" use="required"/>
		<xs:attribute name="time-to-answer" type="idm:kba-time-to-answerType" use="optional"/>
	</xs:complexType>
	<xs:complexType name="kba-answersType">
		<xs:annotation>
			<xs:documentation>The KBA answers submitted by the user.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="answer" type="idm:kba-answerType" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="knowledge-based-authentication-detailsType">
		<xs:annotation>
			<xs:documentation>ComplexType for knowledge based authentication request details</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="answers" type="idm:kba-answersType" minOccurs="0" maxOccurs="1">
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="request">
		<xs:annotation>
			<xs:documentation>Message element that contains a single request
			</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="idm:requestType"/>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="requests">
		<xs:annotation>
			<xs:documentation>Message element that contains a set of requests
			</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="idm:request" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="response">
		<xs:annotation>
			<xs:documentation>Message element that contains a single response
			</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="idm:response-type">
					<xs:attribute name="client-reference" type="xs:string" use="optional">
						<xs:annotation>
							<xs:documentation>Optional identifier that can be used to correlate this transaction with other records. This identifier is returned in the response message.
					</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="enquiry-id" type="xs:token" use="required"/>
					<xs:attributeGroup ref="idm:profile-attributes"/>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="responses">
		<xs:annotation>
			<xs:documentation>Message element that contains a set of responses
			</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="idm:response" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>Message element that contains a single response
						</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="previous-enquiry-identity-details">
		<xs:annotation>
			<xs:documentation>Message element to request identity details on a previous verification enquiry referenced by the enquiry-id
			</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="idm:previous-enquiry-requestType"/>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="identity-details">
		<xs:annotation>
			<xs:documentation>Message element to return the identity details on a previous verification enquiry</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="idm:identity-details-requestType"/>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="previous-enquiry-request">
		<xs:annotation>
			<xs:documentation>Message element to request details on a verification request message referenced by the enquiry-id
			</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="idm:previous-enquiry-requestType"/>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="previous-enquiry-response">
		<xs:annotation>
			<xs:documentation>Message element to request details on a verification response message referenced by the enquiry-id
			</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="idm:previous-enquiry-requestType"/>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
</xs:schema>
